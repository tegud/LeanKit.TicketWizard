               "label":"Identify the specific constraints",
               "options":[
                  {
                     "value":"N/A",
                     "selected":true
                  }
               ]
            },
            {
               "label":"Identify if a new text will be introduced",
               "options":[
                  {
                     "value":"N/A",
                     "selected":true
                  }
               ]
            },
            {
               "label":"Translation of new text",
               "options":[
                  {
                     "value":"N/A",
                     "selected":true
                  }
               ]
            },
            {
               "label":"Identify if the artwork is needed and provided",
               "options":[
                  {
                     "value":"N/A",
                     "selected":true
                  }
               ]
            }
